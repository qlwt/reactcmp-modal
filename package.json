{
    "name": "@qyu/reactcmp-modal",
    "type": "module",
    "version": "1.0.0",
    "main": "./build/bundle/entry/index.js",
    "description": "Generic Modal components for react",
    "keywords": ["react", "component", "element", "modal", "overlay", "animated", "focus"],
    "homepage": "https://github.com/qlwt/reactcmp-modal#readme",
    "license": "MIT",
    "files": [
        "./dist/**/*",
        "./build/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "github:qlwt/reactcmp-modal"
    },
    "scripts": {
        "build": "run-s build:clear build:rollup",
        "build:clear": "rimraf ./build",
        "build:rollup": "rollup -c npm/rollup/rollup.config.js",
        "test": "vitest --config ./npm/vite/vite.config.js"
    },
    "exports": {
        ".": {
            "types": "./build/declaration/index.d.ts",
            "import": "./build/bundle/entry/index.js"
        },
        "./style/*": {
            "types": "./dist/style/*.css",
            "import": "./dist/style/*.d.ts"
        }
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^6.0.1",
        "rollup": "^4.20.0",
        "typescript": "^5.8.2",
        "typescript-transform-paths": "^3.5.5"
    },
    "dependencies": {
        "@qyu/signal-core": "^1.2.0",
        "@qyu/signal-react": "^1.2.2",
        "@qyu/anim-core": "^1.0.2",
        "@qyu/anim-react": "^1.0.4",
        "tslib": "^2.6.3"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    }
}
